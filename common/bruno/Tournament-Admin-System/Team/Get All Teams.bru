meta {
  name: Get All Teams
  type: graphql
  seq: 2
}

post {
  url: {{host}}
  body: graphql
  auth: none
}

body:graphql {
  query
  {
    teams
    {
      id
      name
      tag
      members
      {
        user
        {
          id
          name
        }
        isTeamCaptain
      }
      tournaments
      {
        tournament
        {
          id
          name
        }
      }
    }
  }
}
