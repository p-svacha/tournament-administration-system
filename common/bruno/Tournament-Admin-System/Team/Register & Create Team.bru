meta {
  name: Register & Create Team
  type: graphql
  seq: 1
}

post {
  url: {{host}}
  body: graphql
  auth: none
}

body:graphql {
  mutation {
    registerTeam(data: {
      name: "ArcticGaming",
      tag: "AG",
      userId: 1,
      tournamentId: 3
    })
    {
      success
      message
      team
      {
        id
        name
        tag
        members
        {
          user
          {
            name
          }
        }
      }
    }
  }
}
