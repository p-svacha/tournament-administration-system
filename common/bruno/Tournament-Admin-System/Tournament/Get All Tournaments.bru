meta {
  name: Get All Tournaments
  type: graphql
  seq: 2
}

post {
  url: http://217.154.9.148:3000/graphql
  body: graphql
  auth: none
}

body:graphql {
  query
  {
    tournaments(publishedOnly: false)
    {
      id
      name
      isPublished
      participants
      {
        user
        {
          id
          name
          seat
          isGlobalAdmin
        }
        initialSeed
        finalRank
      }
      event
      {
        name
      }
    }
  }
}
