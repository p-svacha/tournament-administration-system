meta {
  name: Get Tournament
  type: graphql
  seq: 3
}

post {
  url: {{host}}
  body: graphql
  auth: none
}

body:graphql {
  query
  {
    tournament(id: 16)
    {
      id
      name
      participants
      {
        initialSeed
        finalRank
        user
        {
          id
          name
          seat
        }
      }
    }
  }
}
